{
    "development": true,

    "webserver": {
        "port": 8080,

        "allow_headers": [
            "Accept",
            "Authorization",
            "Access-Control-Allow-Headers",
            "Origin",
            "Content-Type",
            "Access-Control-Request-Method",
            "Access-Control-Request-Headers"
        ],
        "allow_methods": [
            "DELETE",
            "GET",
            "HEAD",
            "PATCH",
            "POST",
            "PUT"
        ],
        "origins": [
            "http://localhost:8080"
        ]
    }
}
